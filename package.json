{
  "name": "cleveremote-box",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:webpack:dev": "rimraf dist && nest build  --builder webpack webpack.config.js",
    "build-exe": "esbuild dist/main.js --bundle  --platform=node  --external:farmhash --external:serialport --outfile=main-exe.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug=0.0.0.0:5200 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build-singlefile": "browserify --node --ignore-missing ./dist/src/main.ts | terser > bundle.js"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
